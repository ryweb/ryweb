<% if !['users'].include?(params[:controller]) || !['index'].include?(params[:action])%>
<% if !current_user.nil? && current_user.user_level <= 10 %>
<li><%= link_to 'Käyttäjät', { :controller => 'users', :action => 'list' }, :class => 'icon icon_users' %></li>				
	<% if current_user.user_level <= 5 %>
		<li><%= link_to 'Yhdistykset', customers_path, :class => 'icon icon_places' %></li>
	<% else %>
		<li><%= link_to 'Yhdistyksen tiedot', customer_path(:id => Customer.current.id), :class => 'icon icon_places' %></li>
	<% end %>
<% permitted_to? :create, :ui_templates do %>
  <li><%= link_to 'Ulkoasumallit', ui_templates_path, :class => 'icon icon_themes' %></li>
<% end %>

<% end %>
<% end %>
